{
  "env": {
    "node": true
  },
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "extraFileExtensions": [".vue"]
  },
  "extends": ["eslint:recommended", "plugin:vue/vue3-recommended", "prettier"],
  "plugins": ["@typescript-eslint"],
  "rules": { 
    "no-unused-vars": 0
  }
}
